{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "root": ".",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": ["**/*.json", "**/*.md"],
    "tsConfigPath": "tsconfig.json",
    "webpack": false
  }
}
